# 生產環境變數

# 應用配置
APP_NAME=Electron Vite React # 應用名稱
APP_VERSION=0.1.0 # 版本
APP_LOG_LEVEL=error # 日誌等級

# 服務設定
API_URL=https://api.example.com/api # API URL
API_TIMEOUT=10000 # API 超時時間

# 功能開關
FEATURE_EXPERIMENTAL=false # 實驗性功能
ENABLE_DEVTOOLS=false # 開發者工具
ENABLE_CRASH_REPORTER=true # 崩潰報告

# 視窗設定
WINDOW_WIDTH=1024 # 視窗寬度
WINDOW_HEIGHT=768 # 視窗高度
WINDOW_MIN_WIDTH=800 # 最小寬度
WINDOW_MIN_HEIGHT=600 # 最小高度